osition: right bottom;\n  -webkit-animation: swing-in-top-fwd 2s cubic-bezier(0.175, 0.885, 0.32, 1.275)\n    both;\n  animation: swing-in-top-fwd 2s cubic-bezier(0.175, 0.885, 0.32, 1.275) both;\n  transition: all 500ms ease;\n}\n\n.btn:hover {\n  background-position: left bottom;\n  opacity: 0.9;\n  color: $main-color;\n  cursor: pointer;\n}\n\n.fab {\n  font-size: 30px;\n  color: $secondary-color;\n  transition: all 500ms;\n}\n\n.fa:hover,\n.fab:hover {\n  color: $tint-color;\n  cursor: pointer;\n}\n\n#profile-pic {\n  border-radius: 100%;\n  border: 5px solid $tint-color;\n  height: 40vh;\n}\n\n#logo {\n  height: 40vh;\n  margin-top: -30px;\n}\n\n.left {\n  display: flex;\n  flex-direction: column;\n  width: 60vw;\n}\n\n.item,\n.menu {\n  height: 7vh;\n  color: $main-color;\n  font-size: 1.5rem;\n  border-bottom: 4px solid $main-color;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: flex-start;\n  background: linear-gradient(to bottom, $secondary-color 50%, $tint-color 50%);\n  background-size: 100% 205%;\n  background-position: top left;\n  transition: all 300ms ease-in-out;\n  overflow: hidden;\n  padding: 0 15px;\n}\n\n.item:hover,\n.menu:hover {\n  cursor: pointer;\n  background-position: left bottom;\n}\n\n.item h2,\n.menu h2 {\n  margin-top: 5px;\n  font-size: 1.5rem;\n  font-weight: 500;\n}\n\n.item p {\n  font-weight: 300;\n  font-size: 1.25rem;\n  color: $secondary-color;\n  margin-bottom: 15px;\n}\n\n.selected {\n  height: 79vh;\n  background: $main-color;\n  color: $tint-color;\n  justify-content: center;\n}\n.selected h2 {\n  margin-top: 25px;\n}\n\n.item a {\n  text-decoration: none;\n  color: $tint-color;\n  transition: all 300ms ease-in-out;\n  height: 100%;\n}\n\n.highlight-gif {\n  width: 100%;\n  position: relative;\n}\n\n.hidden {\n  opacity: 0;\n}\n\n.lower-content {\n  bottom: 20vh;\n}\n\nh1 {\n  background: $tint-color;\n  margin-top: -200px;\n  z-index: 100;\n}\n\n#portfolio,\n#blog,\n#music,\n#youtube {\n  background-repeat: no-repeat !important;\n  background-size: cover;\n  background-position: center !important;\n  transition: all 300ms linear;\n}\n\n#portfolio {\n  background: -webkit-linear-gradient($off-black, $off-black),\n    url(\"assets/portfolio.gif\");\n}\n\n#blog {\n  background: -webkit-linear-gradient($off-black, $off-black),\n    url(\"assets/blog.gif\");\n  background-position: left !important;\n}\n\n#youtube {\n  background: -webkit-linear-gradient($off-black, $off-black),\n    url(\"assets/youtube.gif\");\n  background-size: cover !important;\n}\n\n#music {\n  background: -webkit-linear-gradient($off-black, $off-black),\n    url(\"assets/music.gif\");\n}\n\n#portfolio:hover,\n#blog:hover,\n#music:hover,\n#youtube:hover {\n  opacity: 0.8;\n}\n\n.link-text {\n  background: $main-color;\n  border-radius: 5px;\n  box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1),\n    0 2px 4px -1px rgba(0, 0, 0, 0.06);\n  padding: 10px;\n  display: flex;\n  justify-content: center;\n  flex-direction: column;\n  transition: 300ms all linear;\n  width: 80%;\n}\n\n.link-text:hover {\n  box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.1),\n    0 10px 10px -5px rgba(0, 0, 0, 0.04);\n  transform: translateY(-10px);\n}\n\n.hide {\n  display: none;\n}\n\n.visit-btn,\n.visit-btn a {\n  background: $tint-color;\n  text-decoration: none;\n  color: $main-color;\n  border-radius: 5px;\n  margin-top: 10px;\n  padding: 5px;\n  font-size: 1.2rem;\n  transition: all 200ms ease-in-out;\n}\n\n.visit-btn:hover,\n.visit-btn a:hover {\n  cursor: pointer;\n  background: $main-color;\n  color: $tint-color;\n  transform: translateY(5px);\n}\n\n@media only screen and (max-width: 800px) {\n  .right,\n  .left {\n    width: 50vw;\n  }\n\n  #youtube {\n    background: -webkit-linear-gradient($off-black, $off-black),\n      url(\"assets/youtube.gif\");\n    background-size: cover !important;\n  }\n}\n\n@media only screen and (max-width: 700px) {\n  .container {\n    display: flex;\n    flex-direction: column-reverse;\n  }\n\n  .left {\n    width: 100vw;\n  }\n\n  .right {\n    width: 100vw;\n  }\n\n  .scroll-down {\n    display: block;\n    background: $secondary-color;\n    color: $off-white;\n    display: flex;\n    place-items: center;\n    justify-content: center;\n    font-size: 1rem;\n    position: absolute;\n    bottom: 0;\n    left: 0;\n    padding: 5px 0;\n    width: 100%;\n    text-align: center;\n    opacity: 1;\n    animation: fade 1.5s linear infinite;\n  }\n\n  #profile-pic {\n    margin-top: -10px;\n    margin-bottom: -10px;\n    height: 40vh;\n  }\n}\n\n@keyframes fade {\n  0%,\n  100% {\n    opacity: 0.4;\n    transform: translateY(-5px);\n    color: $off-white;\n  }\n  50% {\n    opacity: 1;\n    transform: translateY(0px);\n    color: $tint-color;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"81d4b20991aefacd10df707f8e58671c","cacheData":{"env":{}}}